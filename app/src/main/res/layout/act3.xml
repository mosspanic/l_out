<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadingEdge="horizontal|vertical"
    android:layoutMode="opticalBounds"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="204dp"
        android:layout_height="208dp"
        android:fadingEdge="horizontal"
        android:fadingEdgeLength="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="194dp"
            android:layout_height="147dp"
            android:text="TextView"
            android:textSize="34sp" />
    </FrameLayout>

    <ImageButton
        android:id="@+id/imageButton2"
        style="@android:style/Widget.Material.ImageButton"
        android:layout_width="199dp"
        android:layout_height="156dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="61dp"
        app:barrierMargin="100dp"
        app:circularflow_angles="40"
        app:circularflow_defaultAngle="10"
        app:circularflow_defaultRadius="100dp"
        app:circularflow_radiusInDP="20"
        app:circularflow_viewCenter="30"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout"
        app:layout_constraintCircle="20dp"
        app:layout_constraintCircleAngle="20"
        app:layout_constraintCircleRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/killbutint2_round" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/face_foreground"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="532dp" />

</androidx.constraintlayout.widget.ConstraintLayout>